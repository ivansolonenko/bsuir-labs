<!DOCTYPE html>
<html>
<head>
	<title>@ViewBag.Title</title>
	<link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet">
	@*<link href="@Url.Content("~/Content/themes/bootstrap/slate.css")" rel="stylesheet">*@
	@*<link href="@Url.Content("~/Content/themes/bootstrap/united.css")" rel="stylesheet">*@
	<link href="@Url.Content("~/Content/bootstrap-responsive.css")" rel="stylesheet">
	<link href="@Url.Content("~/Content/docs.css")" rel="stylesheet">
</head>
<body>
	<div class="navbar navbar-inverse navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
					<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar">
					</span>
				</button>
				@Html.ActionLink("Timetable Application", "Index", "Default", new { }, new { @class = "brand" })
				<div class="nav-collapse collapse">
					<ul class="nav">
						<li class="">
							@Html.ActionLink("Home", "Index", "Default")
						</li>
						<li class="">
							@Html.ActionLink("View Log", "Log", "Default")
						</li>
						<li class="">
							@Html.ActionLink("Import/Export", "ImportExport", "Default")
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
	<header class="jumbotron subhead">
		<div class="container">
			<h2>
				@ViewBag.Header
			</h2>
			<p class="lead">
				:)
			</p>
		</div>
	</header>
	<div class="container">
		<div id="row">
			<div id="span12">
				@RenderBody()
			</div>
		</div>
	</div>
	<script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
</body>
</html>
